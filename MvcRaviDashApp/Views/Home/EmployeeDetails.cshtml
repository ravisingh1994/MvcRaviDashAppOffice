
@{
    ViewBag.Title = "EmployeeDetails";
}

@model List<MvcRaviDashApp.DB_Employee.EmpChetu>

<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Tables</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Employee Advanced Tables
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>Emp Id</th>
                                        <th>Name</th>
                                        <th>Email</th>
                                        <th>City</th>
                                        <th>Salary</th>
                                    </tr>
                                </thead>
                                @foreach(var item in Model)
                                   {
                            <tbody>
                                <tr class="odd gradeX">
                                    <td>@item.eid</td>
                                    <td>@item.ename</td>
                                    <td>@item.Eemail</td>
                                    <td class="center">@item.City</td>
                                    <td class="center">@item.Salary</td>
                                </tr>
                            </tbody>
                              }
                                
                            </table>
                        </div>
                        <!-- /.table-responsive -->
                      
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        
        <!-- /.row -->
      
        <!-- /.row -->
        
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>

